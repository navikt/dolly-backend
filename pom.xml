<?xml version="1.0" encoding="UTF-8" ?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.0.0.RELEASE</version>
	</parent>

	<groupId>no.nav.dolly-backend</groupId>
	<artifactId>dolly-backend</artifactId>
	<version>1.18.0-SNAPSHOT</version>
	<packaging>pom</packaging>

	<name>dolly-backend</name>
	<description>Applikasjon for oppretting av testpersoner</description>

    <modules>
        <module>dolly-backend-app</module>
        <module>dolly-backend-domain</module>
        <module>dolly-backend-app-local</module>
        <module>dolly-backend-app-nais</module>
    </modules>

    <properties>
        <application.name>dolly-backend</application.name>
        <application.version>${project.version}</application.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <java.version>1.8</java.version>
        <freg-security-oidc.version>3.0.5</freg-security-oidc.version>
        <freg-spring-boot-starters.version>2.0.0</freg-spring-boot-starters.version>
        <springfox.version>2.9.2</springfox.version>
        <kodeverk.version>1.2</kodeverk.version>
        <ojdbc.version>12.2.0.1</ojdbc.version>
        <orika.version>1.5.2</orika.version>
        <cxf.version>3.2.4</cxf.version>
        <modig-security.version>3.0.6</modig-security.version>
        <behandle.arbeidsforhold.version>1.2.1</behandle.arbeidsforhold.version>
        <ehcache-core.version>2.6.11</ehcache-core.version>
        <commons-collections4.version>4.1</commons-collections4.version>
        <jettison.version>1.3.7</jettison.version>
        <junit-platform.version>1.2.0</junit-platform.version>
        <wiremock.version>2.24.0</wiremock.version>
        <spring-cloud-contract.version>2.1.2.RELEASE</spring-cloud-contract.version>
        <freg-metrics.version>1.0.0</freg-metrics.version>
        <freg-security-test-oidc-tools.version>3.2.0</freg-security-test-oidc-tools.version>
        <sonar.exclusions>**/dolly/domain/**, **/dolly/regression/**, **/dolly/exceptions/**,**/main/dolly-web/**,
            **/dolly/testdata/builder/**
        </sonar.exclusions>

        <papertrailapp.version>1.0.0</papertrailapp.version>
        <logstash-logback-encoder.version>5.1</logstash-logback-encoder.version>
    </properties>

    <dependencies>

        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <optional>true</optional>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-security</artifactId>
        </dependency>

        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-lang3</artifactId>
        </dependency>

        <dependency>
            <groupId>net.sf.ehcache</groupId>
            <artifactId>ehcache-core</artifactId>
            <version>${ehcache-core.version}</version>
        </dependency>

        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-collections4</artifactId>
            <version>${commons-collections4.version}</version>
        </dependency>
    </dependencies>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>net.logstash.logback</groupId>
                <artifactId>logstash-logback-encoder</artifactId>
                <version>${logstash-logback-encoder.version}</version>
            </dependency>

            <dependency>
                <groupId>no.nav.freg</groupId>
                <artifactId>freg-security-oidc</artifactId>
                <version>${freg-security-oidc.version}</version>
            </dependency>

            <dependency>
                <groupId>no.nav.freg</groupId>
                <artifactId>freg-spring-boot-starter-log-exceptions</artifactId>
                <version>${freg-spring-boot-starters.version}</version>
            </dependency>

            <dependency>
                <groupId>no.nav.tjenester</groupId>
                <artifactId>kodeverk-api-v1</artifactId>
                <version>${kodeverk.version}</version>
            </dependency>

            <dependency>
                <groupId>io.springfox</groupId>
                <artifactId>springfox-swagger2</artifactId>
                <version>${springfox.version}</version>
            </dependency>

            <dependency>
                <groupId>io.springfox</groupId>
                <artifactId>springfox-swagger-ui</artifactId>
                <version>${springfox.version}</version>
            </dependency>

            <dependency>
                <groupId>com.oracle</groupId>
                <artifactId>ojdbc8</artifactId>
                <version>${ojdbc.version}</version>
            </dependency>

            <dependency>
                <groupId>com.oracle</groupId>
                <artifactId>ucp</artifactId>
                <version>${ojdbc.version}</version>
            </dependency>

            <dependency>
                <groupId>com.oracle</groupId>
                <artifactId>ons</artifactId>
                <version>${ojdbc.version}</version>
            </dependency>

			<dependency>
				<groupId>ma.glasnost.orika</groupId>
				<artifactId>orika-core</artifactId>
				<version>${orika.version}</version>
			</dependency>

            <dependency>
                <groupId>org.codehaus.jettison</groupId>
                <artifactId>jettison</artifactId>
                <version>${jettison.version}</version>
            </dependency>

            <dependency>
                <groupId>org.apache.cxf</groupId>
                <artifactId>cxf-spring-boot-starter-jaxws</artifactId>
                <version>${cxf.version}</version>
            </dependency>

            <dependency>
                <groupId>org.apache.cxf</groupId>
                <artifactId>cxf-rt-frontend-jaxws</artifactId>
                <version>${cxf.version}</version>
            </dependency>

            <dependency>
                <groupId>no.nav.modig</groupId>
                <artifactId>modig-security-ws</artifactId>
                <version>${modig-security.version}</version>
            </dependency>

			<dependency>
				<groupId>no.nav.tjenester.domene.arbeidsforhold</groupId>
				<artifactId>nav-fim-behandleArbeidsforhold-v1-tjenestespesifikasjon</artifactId>
				<version>${behandle.arbeidsforhold.version}</version>
				<classifier>jaxws</classifier>
			</dependency>

            <dependency>
                <groupId>no.nav.freg</groupId>
                <artifactId>metrics-spring</artifactId>
                <version>${freg-metrics.version}</version>
            </dependency>

            <dependency>
                <groupId>no.nav.freg</groupId>
                <artifactId>metrics-annotations</artifactId>
                <version>${freg-metrics.version}</version>
            </dependency>
		</dependencies>

	</dependencyManagement>


</project>
