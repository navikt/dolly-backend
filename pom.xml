<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.0.0.RELEASE</version>
	</parent>

	<groupId>no.nav.dolly</groupId>
	<artifactId>dolly</artifactId>
	<version>test-1131</version>
	<packaging>pom</packaging>

	<name>dolly</name>
	<description>Applikasjon for oppretting av testpersoner</description>

	<modules>
		<module>dolly-app</module>
		<module>dolly-domain</module>
		<module>dolly-webapp</module>
		<module>dolly-test</module>
		<module>dolly-test-common</module>
    </modules>

	<properties>
		<application.name>dolly</application.name>
		<application.version>${project.version}</application.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<java.version>1.8</java.version>
		<freg-security-oidc.version>3.0.5</freg-security-oidc.version>
		<freg-spring-boot-starters.version>2.0.0</freg-spring-boot-starters.version>
		<springfox-swagger2.version>2.4.0</springfox-swagger2.version>
		<springfox-swagger-ui.version>2.4.0</springfox-swagger-ui.version>
		<kodeverk.version>1.2</kodeverk.version>
		<logstash-logback-encoder.version>5.1</logstash-logback-encoder.version>
        <ojdbc.version>12.2.0.1</ojdbc.version>
		<android.json.version>0.0.20131108.vaadin1</android.json.version>
		<orika.version>1.5.2</orika.version>
		<cxf.version>3.2.4</cxf.version>
		<modig-security.version>3.0.6</modig-security.version>
		<behandle.arbeidsforhold.version>1.2.1</behandle.arbeidsforhold.version>
		<sonar.exclusions>**/dolly/domain/**, **/dolly/regression/**, **/dolly/exceptions/**,**/main/dolly-web/**, **/dolly/testdata/builder/**</sonar.exclusions>
	</properties>

	<dependencies>

		<!-- Automatisk eksludert ved repackage -->
		<!--
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-devtools</artifactId>
			<optional>true</optional>
		</dependency>
		-->

		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<optional>true</optional>
		</dependency>

		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-security</artifactId>
		</dependency>

		<dependency>
			<groupId>org.apache.commons</groupId>
			<artifactId>commons-lang3</artifactId>
		</dependency>

		<dependency>
			<groupId>net.sf.ehcache</groupId>
			<artifactId>ehcache-core</artifactId>
			<version>LATEST</version>
		</dependency>

		<dependency>
			<groupId>org.apache.commons</groupId>
			<artifactId>commons-collections4</artifactId>
			<version>4.1</version>
		</dependency>
	</dependencies>

	<dependencyManagement>
        <dependencies>
			<dependency>
				<groupId>no.nav.freg</groupId>
				<artifactId>freg-security-oidc</artifactId>
				<version>${freg-security-oidc.version}</version>
			</dependency>

			<dependency>
				<groupId>no.nav.freg</groupId>
				<artifactId>freg-spring-boot-starter-log-exceptions</artifactId>
				<version>${freg-spring-boot-starters.version}</version>
			</dependency>

			<dependency>
				<groupId>no.nav.tjenester</groupId>
				<artifactId>kodeverk-api-v1</artifactId>
				<version>${kodeverk.version}</version>
			</dependency>

			<dependency>
				<groupId>net.logstash.logback</groupId>
				<artifactId>logstash-logback-encoder</artifactId>
				<version>${logstash-logback-encoder.version}</version>
			</dependency>

			<dependency>
				<groupId>io.springfox</groupId>
				<artifactId>springfox-swagger2</artifactId>
				<version>${springfox-swagger2.version}</version>
			</dependency>

			<dependency>
				<groupId>io.springfox</groupId>
				<artifactId>springfox-swagger-ui</artifactId>
				<version>${springfox-swagger-ui.version}</version>
			</dependency>

			<dependency>
				<groupId>com.oracle</groupId>
				<artifactId>ojdbc8</artifactId>
				<version>${ojdbc.version}</version>
			</dependency>

			<dependency>
				<groupId>com.oracle</groupId>
				<artifactId>ucp</artifactId>
				<version>${ojdbc.version}</version>
			</dependency>

			<dependency>
				<groupId>com.oracle</groupId>
				<artifactId>ons</artifactId>
				<version>${ojdbc.version}</version>
			</dependency>

			<dependency>
				<groupId>ma.glasnost.orika</groupId>
				<artifactId>orika-core</artifactId>
				<version>${orika.version}</version>
			</dependency>

			<dependency>
				<groupId>com.vaadin.external.google</groupId>
				<artifactId>android-json</artifactId>
				<version>${android.json.version}</version>
			</dependency>

			<dependency>
				<groupId>org.apache.cxf</groupId>
				<artifactId>cxf-spring-boot-starter-jaxws</artifactId>
				<version>${cxf.version}</version>
			</dependency>

			<dependency>
				<groupId>org.apache.cxf</groupId>
				<artifactId>cxf-rt-frontend-jaxws</artifactId>
				<version>${cxf.version}</version>
			</dependency>

			<dependency>
				<groupId>no.nav.modig</groupId>
				<artifactId>modig-security-ws</artifactId>
				<version>${modig-security.version}</version>
			</dependency>

			<dependency>
				<groupId>no.nav.tjenester.domene.arbeidsforhold</groupId>
				<artifactId>nav-fim-behandleArbeidsforhold-v1-tjenestespesifikasjon</artifactId>
				<version>${behandle.arbeidsforhold.version}</version>
				<classifier>jaxws</classifier>
			</dependency>
		</dependencies>

	</dependencyManagement>


</project>
